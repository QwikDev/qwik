{
  "name": "qwik-site",
  "version": "0.0.0",
  "description": "Qwik Site.",
  "scripts": {
    "build": "npm run build.client && npm run build.server",
    "build.client": "vite build",
    "build.server": "vite build --outDir server/build --ssr src/entry.cloudflare.tsx",
    "preview": "vite preview",
    "dev": "npm run dev.ssr",
    "dev.ssr": "vite",
    "dev.client": "vite --mode clientonly",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "start": "npm run dev",
    "test": "uvu -r tsm qwest/vite/tests",
    "link.builder": "npm link @builder.io/qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.4.2",
    "@builder.io/qwik": "0.0.17-0-dev20220303165846",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "^3.2.0",
    "@cloudflare/wrangler": "1.19.7",
    "@mdx-js/rollup": "^2.0.0",
    "@types/marked": "^4.0.2",
    "@types/mdx": "^2.0.1",
    "@types/service-worker-mock": "2.0.1",
    "autoprefixer": "10.4.2",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "postcss": "8.4.6",
    "prettier": "2.3.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^1.1.1",
    "slugify": "^1.6.5",
    "tailwindcss": "3.0.18",
    "tsm": "^2.2.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.3",
    "vite": "^2.7.13",
    "node-fetch": "2.x"
  },
  "author": "Builder.io Team",
  "engines": {
    "node": ">=14.14.0"
  },
  "license": "MIT"
}
