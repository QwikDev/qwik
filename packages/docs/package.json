{
  "name": "qwik-docs",
  "version": "0.0.1",
  "description": "Qwik Docs Site",
  "homepage": "https://qwik.builder.io/",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "contributors": "tsm contributors.ts",
    "dev": "vite --mode ssr --open",
    "dev.debug": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
    "preview": "qwik build preview && vite preview --open",
    "preview.wrangler": "wrangler pages dev ./dist",
    "build.showcase": "pnpm node scripts/showcase.js",
    "start": "pnpm dev",
    "codesandbox.sync": "tsm codesandbox.sync.ts",
    "deploy": "wrangler pages publish ./dist"
  },
  "devDependencies": {
    "@algolia/autocomplete-core": "1.7.4",
    "@algolia/client-search": "4.14.3",
    "@builder.io/partytown": "0.8.0",
    "@builder.io/qwik": "github:BuilderIo/qwik-build#main",
    "@builder.io/qwik-city": "github:BuilderIo/qwik-city-build#main",
    "@builder.io/qwik-react": "0.5.0",
    "@builder.io/sdk-qwik": "^0.4.1",
    "@docsearch/css": "3.3.4",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.0",
    "@mui/x-data-grid": "^6.4.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "algoliasearch": "4.16.0",
    "autoprefixer": "10.4.14",
    "fflate": "0.8.0",
    "gray-matter": "4.0.3",
    "postcss": "8.4.24",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "puppeteer": "^20.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "^0.9.6",
    "shiki": "^0.14.2",
    "tailwindcss": "3.3.2",
    "tsm": "2.3.0",
    "typescript": "5.1.3",
    "undici": "5.22.1",
    "uvu": "0.5.6",
    "vite": "4.3.9",
    "vite-plugin-inspect": "^0.7.28",
    "wrangler": "^3.1.0"
  },
  "author": "Builder.io Team",
  "bugs": {
    "url": "https://github.com/BuilderIO/qwik"
  },
  "packageManager": "pnpm@8.6.1",
  "engines": {
    "node": ">=18.11",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.0.0"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@modular-forms/qwik": "^0.12.0",
    "@supabase/supabase-js": "^2.24.0",
    "@unpic/core": "^0.0.24",
    "@unpic/qwik": "^0.0.19",
    "openai": "^3.2.1",
    "qwik-image": "^0.0.6",
    "snarkdown": "^2.0.0"
  }
}
