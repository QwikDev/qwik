{
  "name": "@qwik.dev/react",
  "description": "Qwik React allows adding React components into existing Qwik application",
  "version": "2.0.0-alpha.6",
  "bugs": "https://github.com/QwikDev/qwik/issues",
  "devDependencies": {
    "@qwik.dev/core": "workspace:*",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.4.5",
    "vite": "5.4.10"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.qwik.d.ts",
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    },
    "./vite": {
      "types": "./lib/types/vite.d.ts",
      "import": "./lib/vite.mjs",
      "require": "./lib/vite.cjs"
    }
  },
  "files": [
    "lib",
    "vite"
  ],
  "homepage": "https://qwik.dev/",
  "license": "MIT",
  "main": "./lib/index.qwik.mjs",
  "peerDependencies": {
    "@qwik.dev/core": "workspace:^",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "vite": "^5"
  },
  "qwik": "./lib/index.qwik.mjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QwikDev/qwik.git",
    "directory": "packages/qwik-react"
  },
  "scripts": {
    "build": "npm run build.lib",
    "build.client": "vite build",
    "build.lib": "vite build --mode lib",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
    "dev.ssr": "vite --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "pnpm run -w build.local && pnpm publish",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "./lib/types/index.qwik.d.ts"
}
